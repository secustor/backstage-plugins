{
  "permissions": {
    "defaultMode": "acceptEdits",
    "allow": [
      "Bash(chmod +x:*)",
      "Bash(echo:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(head:*)",
      "Bash(yarn build:*)",
      "Bash(yarn check)",
      "Bash(yarn fix)",
      "Bash(yarn jest:*)",
      "Bash(yarn lint:*)",
      "Bash(yarn lint:fix:*)",
      "Bash(yarn prettier:fix:*)",
      "Bash(yarn test:*)",
      "Bash(yarn test:all:*)",
      "Bash(yarn test:jest:*)",
      "Bash(yarn tsc:*)",
      "Bash(yarn tsc:full:*)",
      "Bash(yarn workspace:*)",
      "WebFetch(domain:mise.jdx.dev)",
      "WebFetch(domain:ui.backstage.io)"
    ],
    "deny": []
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "yarn run check"
          },
          {
            "type": "command",
            "command": "yarn run dedupe --check"
          }
        ]
      }
    ]
  }
}
